# LOOPBACK
auto lo
iface lo inet loopback

# ETH0 - UPSTREAM
auto eth0
iface eth0 inet dhcp

# ETH1 - Staff network
auto eth1
iface eth1 inet static
	address 172.18.0.1
	network 172.18.0.0
	netmask 255.255.252.0

# VLAN 103 - Competitor network
auto eth2
iface eth2 inet static
	address 172.19.0.1
	network 172.19.0.0
	netmask 255.255.252.0
